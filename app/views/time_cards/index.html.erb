<h1>タイムカード一覧画面<%= @year %>年<%= @month %>月度勤務一覧</h1>
  <table class="table, table-bordered, table-condensed">
  	<thead>
  		<tr>
  			<th>日付</th>
  			<th>曜</th>
  			<th>出社時間</th>
  			<th>退社時間</th>
  			<th>勤務時間</th>
  		</tr>
  	</thead>
  <% @times.each do |date| %>
  	<tbody>
  		<tr>
  			<td><%= date.date.day %></td>
  			<td></td>
        <% if date.blank? %>
  			<td></td>
  			<td></td>
  			<td></td>
        <% else %>
  			<td><%= date.in_at %></td>
  			<td><%= date.out_at %></td>
  			<td><%= date.work_hours_in_seconds%></td>
  	    <%end%>
  		</tr>
  	</tbody>
  <% end %>
  </table>
